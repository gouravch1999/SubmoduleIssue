<?xml version="1.0" encoding="UTF-8"?>
<BUILD_DEF xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="Build.xsd" SCHEMA_VERSION="1.15.0" ERROR_HANDLING="ABORT">
  <CONFIG>.\HGIF_SiMa_Config.xml</CONFIG>
  <STAGE_CREATE ENABLED="TRUE" ERROR_HANDLING="ABORT">
    <INPUT_VALIDATOR/>
    <PRE_IMPORT/>
    <IMPORT ENABLED="TRUE" ERROR_HANDLING="ABORT">
      <BUS_SIGNAL_IMPORTS/>
      <SYS_SIGNAL_IMPORTS/>
    </IMPORT>
    <UNIT_HARMONIZATION>
      <UNIT_HARMONIZATION_JOB NAME="UnitHarmonization"/>
    </UNIT_HARMONIZATION>
    <POST_IMPORT/>
  </STAGE_CREATE>
  <STAGE_MAPPING>
    <PRE_MAPPING/>
    <AUTO_MAPPING/>
    <POST_MAPPING/>
    <CE>
      <CE_JOB NAME="CE"/>
    </CE>
  </STAGE_MAPPING>
  <STAGE_GENERATION>
    <PRE_GENERATION/>
    <GENERATORS>
      <CODE JOB_NAME="code"/>
      <DOCUMENT JOB_NAME="doc"/>
      <FARM JOB_NAME="farm"/>
    </GENERATORS>
    <POST_GENERATORS/>
  </STAGE_GENERATION>
</BUILD_DEF>